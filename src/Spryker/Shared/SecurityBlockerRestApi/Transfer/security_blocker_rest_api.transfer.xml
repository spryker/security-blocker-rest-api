<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="RestErrorCollection">
        <property name="restErrors" type="RestErrorMessage[]" singular="restError"/>
    </transfer>

    <transfer name="RestErrorMessage">
        <property name="code" type="string"/>
        <property name="detail" type="string"/>
        <property name="status" type="int"/>
    </transfer>

    <transfer name="SecurityCheckAuthContext">
        <property name="type" type="string"/>
        <property name="ip" type="string"/>
        <property name="account" type="string"/>
    </transfer>

    <transfer name="SecurityCheckAuthResponse">
        <property name="isBlocked" type="bool"/>
        <property name="blockedFor" type="int"/>
    </transfer>

    <transfer name="RestAccessTokensAttributes">
        <property name="username" type="string" restRequestParameter="required"/>
    </transfer>

    <transfer name="RestAgentAccessTokensRequestAttributes">
        <property name="username" type="string" restRequestParameter="required"/>
    </transfer>

</transfers>
